# Maven 
mvnall() {
    if [[ $@ =~ "[cC]" ]]; then
        cg; if mvnp clean install; then :; else dev; return $?; fi;
    fi
    if [[ $@ =~ "[qQ]" ]]; then
        qg; if mvnp clean install; then :; else dev; return $?; fi;
    fi
    if [[ $@ =~ "[vV]" ]]; then
        vg; if mvnp clean install; then :; else dev; return $?; fi;
    fi
    if [[ $@ =~ "[mM]" ]]; then
        mg; if mvnp clean install; then :; else dev; return $?; fi;
    fi
    if [[ $@ =~ "[sS]" ]]; then
        wg; if mvnp -pl code/db -am -P plsql clean package; then :; else dev; return $?; fi;
    fi
    if [[ $@ =~ "[wW]" ]]; then
        wg; if mvnp -pl code/valuations -am clean install; then :; else dev; return $?; fi;
    fi
    if [[ $@ =~ "[iI]" ]]; then
        wgi; if mvnp clean install; then :; else dev; return $?; fi;
    fi
    dev
}
